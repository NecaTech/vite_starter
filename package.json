{
  "name": "mon-projet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/ensure-dirs.js",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "type-check": "tsc --noEmit",
    "init:src": "node scripts/ensure-src-dirs.js",
    "init:index": "node scripts/create-index-files.js"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "postcss": "^8.4.49",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "framer-motion": "^11.11.17",
    "gsap": "^3.12.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.1",
    "tailwind-merge": "^2.5.4"
  }
}
